# define FD_CHECK(x) if (x == -1) {ft_error(); return (1);}
# define TTETRI(x) if (x == NULL) {ft_error(); return (1);}
# define TFILE(x) if (x == -1) return (1);
# define FT_USAGE(x) {ft_usage(x); return (1);}
# define FT_ERRPORM() {ft_error(); return (-1);}
# define FT_ERROR1() {ft_error(); return (1);}
# define MALLOC_CHECK_INT(x) if (x == NULL) return (1);